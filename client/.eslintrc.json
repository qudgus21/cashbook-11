{
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended"], 
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    }, 
    "parser": "babel-eslint",
    "ignorePatterns": ["node_modules/", "dist/", "webpack.config.js"],
    "rules": {
        "prettier/prettier": "error"
    }
}